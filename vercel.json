{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "HEAD"],
      "status": 200,
      "dest": "dist/index.html"
    },
    {
      "src": "/(?!.*\\.)(.*)",
      "dest": "/index.html"
    }
  ]
}
